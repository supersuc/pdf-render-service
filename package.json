{
  "name": "pdf-render-service",
  "version": "2.0.0",
  "description": "企业级 HTML 转 PDF 服务",
  "main": "app.js",
  "scripts": {
    "start": "pm2 start pm2.config.js --env production",
    "restart": "pm2 restart pdf-render-service",
    "stop": "pm2 stop pdf-render-service",
    "delete": "pm2 delete pdf-render-service",
    "logs": "pm2 logs pdf-render-service",
    "dev": "nodemon app.js",
    "test": "node test/test-api.js"
  },
  "keywords": [
    "pdf",
    "html-to-pdf",
    "puppeteer",
    "express"
  ],
  "author": "suchao",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "morgan": "^1.10.0",
    "puppeteer": "^21.6.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "pm2": "^5.3.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
